CREATE TABLE ticker (
  symbol TEXT NOT NULL,
  last_price REAL NOT NULL,
  price_change REAL NOT NULL,
  price_change_percent REAL NOT NULL
);

CREATE INDEX ticker_symbol ON ticker(symbol);

insertItem:
INSERT OR REPLACE
INTO ticker (symbol, last_price, price_change, price_change_percent)
VALUES (?, ?, ?, ?);

selectAll:
SELECT * FROM ticker;

deleteAll:
DELETE FROM ticker;